hello:
	gcc shell.c -o shell
	echo "compiled shell"
	read a
	gcc ls.c -o ls 
	echo "compiled ls"
	read b
	gcc cat.c -o cat
	echo "compiled cat"
	read c
	gcc mkdir.c -o mkdir
	echo "compiled mkdir"
	read d
	gcc rm.c -o rm
	echo "compiled rm"
	read f
	gcc date.c -o date
	echo "compiled date"
	read g
	echo "running shell now..\n"
	./shell
	echo "executed"